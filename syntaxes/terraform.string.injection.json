{
	"scopeName": "source.terraform.string.injection",
	"injectionSelector": "L:source.terraform",
	"patterns": [
		{
			"match": "git::ssh://git@.*\\.git",
			"name": "string.terraform"
		},
		{
			"match": "depends_on",
			"name": "depends.terraform"
		},
		{
			"match": "\\\".*(:).*\\\"",
			"captures": {
				"0": {
					"name": "string.terraform"
				},
				"1": {
					"name": "string.terraform"
				}
			}
		}
	]
}