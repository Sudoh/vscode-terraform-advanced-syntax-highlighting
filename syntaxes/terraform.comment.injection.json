{
	"scopeName": "source.terraform.comment.injection",
	"injectionSelector": "L:comment.line.number-sign.terraform",
	"patterns": [
		{
			"match": "git::ssh://git@.*\\.git",
			"name": "comment.line.number-sign.terraform"
		},
		{
			"match": "depends_on",
			"name": "comment.line.number-sign.terraform"
		},
		{
			"match": "\\\".*(:).*\\\"",
			"captures": {
				"0": {
					"name": "comment.line.number-sign.terraform"
				},
				"1": {
					"name": "comment.line.number-sign.terraform"
				}
			}
		}
	]
}